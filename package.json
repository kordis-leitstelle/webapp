    "@testing-library/svelte": "^3.1.3",
    "@tsconfig/svelte": "3.0.0",
    "@types/mapbox-gl": "2.7.3",
    "@typescript-eslint/eslint-plugin": "5.30.6",
    "autoprefixer": "10.4.7",
    "eslint": "8.19.0",
    "c8": "^7.11.3",
    "eslint-plugin-import": "2.26.0",
    "eslint-plugin-svelte3": "4.0.0",
    "husky": "8.0.1",
    "lint-staged": "13.0.3",
    "jsdom": "^20.0.0",
    "postcss": "8.4.14",
    "postcss-load-config": "3.1.4",
    "prettier": "2.7.1",
